filepath +:./scene5/exoKepler

include exoplanets.speck
eval psize 25000
#eval psize 10
eval ptsize 0.05 2.5
eval points on
eval lum const 10
eval cmap exoKepler/colors.cmap
eval color method exact
eval polylumvar point-size area	
